# FastKit API Environment Configuration
# Copy this file to .env and modify as needed for your environment

#---------------------------------------------------------------
# Database Configuration
#---------------------------------------------------------------

# PostgreSQL connection string
# Format: postgres://<user>:<password>@<host>:<port>/<db_name>?sslmode=disable
DB_URL=postgres://postgres:secret@localhost:5432/mydb?sslmode=disable

#---------------------------------------------------------------
# Migration Configuration
#---------------------------------------------------------------

# Path to migration files (relative to project root)
MIGRATIONS_DIR=manifest/migrations
